{
	"name": "obsidian-attachment-organizer",
	"version": "1.0.1",
	"description": "Obsidian plugin to automatically organize attachments into zone-based folders",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "eslint src",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "plugin", "attachments", "organizer"],
	"author": "Kirun",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^8.51.0",
		"@typescript-eslint/parser": "^8.51.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^9.39.2",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^5.0.4"
	}
}
